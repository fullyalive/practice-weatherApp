{
  "expo": {
    "name": "weather-app",
    "description": "This project is really great.",
    "slug": "weather-app",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "co.fullyalive.weather"
    },
    "android": {
      "package": "co.fullyalive.weather"
    }
  }
}
